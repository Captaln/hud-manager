{
  "name": "hud-manager",
  "version": "0.1.0",
  "homepage": "./",
  "description": "HUD Manager",
  "author": "0sh10",
  "private": true,
  "dependencies": {
    "@node-steam/vdf": "^2.1.0",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "big-integer": "^1.6.47",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "csgogsi": "^1.1.3",
    "csgogsi-socket": "^1.1.2",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "nedb": "^1.8.0",
    "node-fetch": "^2.6.0",
    "pug": "^2.0.4",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.1.2",
    "react-select-country-list": "^2.0.1",
    "reactstrap": "^8.1.1",
    "registry-js": "^1.9.0",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "steam-game-path": "0.0.5",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3",
    "ws": "^7.2.0"
  },
  "build": {
    "appId": "pl.osztenkurden.hud.manager",
    "productName": "HUD MANAGER",
    "win": {
      "target": "NSIS"
    },
    "directories": {
      "output": "app",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "package.json",
      {
        "from": "dist",
        "to": "build",
        "filter": "**/*"
      },
      "!**/*.asar",
      "!**/app/**/*",
      "!**/app/*",
      "!**/win-unpacked/*",
      "!**/.git/**/*",
      "!**/.git/*"
    ]
  },
  "main": "dist/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run transpile && react-scripts build && mv build dist",
    "test": "react-scripts test",
    "transpile": "tsc electron.ts --outDir dist --esModuleInterop true",
    "electron": "npm run transpile && cross-env DEV=true electron ./",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"npm run electron\"",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "compile": "npm run build && npm run pack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/ip": "^1.1.0",
    "@types/nedb": "^1.8.9",
    "@types/node": "^12.11.7",
    "@types/reactstrap": "^8.0.6",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "concurrently": "^4.1.2",
    "electron": "^6.1.2",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.6",
    "wait-on": "^3.3.0"
  }
}
